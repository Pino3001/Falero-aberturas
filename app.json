{
  "expo": {
    "name": "Carpintelab",
    "slug": "Carpinteria",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "carpintelab",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "assets/images/*",
      "assets/fonts/*",
      "node_modules/expo-print/example/assets/*"
    ],
    "splash": {
      "image": "./assets/images/adaptive-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#888"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ],
      "package": "com.pinoDev.Carpintelab",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-adaptive.png",
        "backgroundColor": "#RRGGBB"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-sqlite",
        {
          "enableFTS": true,
          "useSQLCipher": true,
          "android": {
            "enableFTS": false,
            "useSQLCipher": false
          },
          "ios": {
            "customBuildFlags": [
              "-DSQLITE_ENABLE_DBSTAT_VTAB=1 -DSQLITE_ENABLE_SNAPSHOT=1"
            ]
          }
        }
      ],
      "expo-asset",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "07c18041-45f5-4d81-8e21-deb1839137fe"
      }
    },
    "platforms": [
      "android",
      "ios",
      "web"
    ],
    "jsEngine": "hermes",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#106c6c"
    }
  }
}
